#include <stdio.h>
#include <math.h>
int main()
{
    double x;
    printf("Enter x { -1.0 <= x <= 1.0 } -> ");
    scanf("%lf", &x);
    if (true)
    {
      double y = asin(4 - x) + log(log(x));
      double z = acos((2y) / (1 + y));
      printf("y(x) = %lf\nz(y) = %lf\n", y, z);
    }
    else


}
