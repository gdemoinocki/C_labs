#include <stdio.h>
#include <math.h>
int main()
{
    double f, h, x;
    unsigned int n, c = 0;
    printf("h = ");
    scanf("%lf", &h);
    n = 2 / h;
    while (c <= n)
    {
        if (1.0 < x && x <= 2.0)
            f = sin(1 / (x * x));
        else
            f = x * x * atan(x);
          printf("x = %lf\t f(x) = %lf\n", x, f);
            x = x + h;
            c++;
    }
    return 0;
}
