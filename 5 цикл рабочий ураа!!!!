#include <stdio.h>
  int main()
  {
  int p = 1;
  unsigned int (n);
    printf(" Enter number -> \n ");
    scanf("%u", &n);
    while (n > p)
    {
      p = p * 2;
    }
    if (p > 1 & p == n)
    {
        printf("Cycle: number is power of 2\n");
    }
    else
    {
        printf("Cycle: number is not power of 2\n");
    }
   return 0;
}
