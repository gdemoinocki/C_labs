#include <math.h>
#include <stdio.h>

void recur(int n);

int main()
{
    int n;
    fprintf(stdout, "Введите число ");
    fscanf(stdin, "%d", &n);
    recur(n);
    puts("Рекурсия = \n");
}

void recur(int n)
{
  fprintf(stdout, "%d\n", n);
  if (n > 0 && (n & (n-1)) == 0)
    return;
  else
    recur(n);
}
